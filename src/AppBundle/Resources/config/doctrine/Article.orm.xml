<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Article" table="article">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="url" type="string" column="url" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="publishDate" type="datetime" column="publishDate" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="urlToImage" type="string" column="urlToImage" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="author" type="string" column="author" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="articleId" type="string" column="articleId" length="255" nullable="false">
        <options>
            <option name="fixed"/>
        </options>
    </field>
    <field name="planet" type="string" column="planet" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
