{% extends 'base.html.twig' %}
{% block body %}
    <table class="table table-striped">
        <thead>
        <tr>
            <th>Name</th>
            <th>Title</th>
            <th> Description</th>
        </tr>
        </thead>
        <tbody>
        {% for planet in planets %}
            <tr>
                <td>{{ planet.name }}</td>
                <td> {{planet.title}} </td>
                <td> {{ planet.description }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}